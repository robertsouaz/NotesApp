<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NotesApp.AboutPage"
             Title="AboutPage">
    <VerticalStackLayout>
        <Image 
            x:Name ="PerfilNasa"
            Source="eu.jpg"
            Aspect="AspectFit"
            HeightRequest="250"/>
        <Button
            x:Name="PerfilBotao"
            Text="Visite perfil"
            Clicked="PerfilBotao_Clicked"
            HorizontalOptions="Center"/>

    </VerticalStackLayout>
</ContentPage>